{
  "$id": "https://openbelasting.nl/schemas/openblasting.belastingplichtige.schema.json",
  "$schema": "https://docs.commongateway.nl/schemas/Entity.schema.json",
  "version": "0.1",
  "title": "Belastingplichtige",
  "description": "burgerservicenummer van de belastingplichtige die betrokken is bij deze belastingaanslag.",
  "type": "object",
  "required": [
    "burgerservicenummer"
  ],
  "properties": {
    "burgerservicenummer": {
      "pattern": "[0-9]{9}",
      "type": "string",
      "description": "burgerservicenummer",
      "example": "123456789",
      "searchable": true
    }
  }
}
