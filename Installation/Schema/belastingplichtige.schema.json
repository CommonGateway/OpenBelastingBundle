  "$schema": "https://docs.commongateway.nl/schemas/Entity.schema.json",
  "version": "0.1",
  "title": "Belastingplichtige",
  "required": ["burgerservicenummer"],
  "type": "object",
  "properties": {
    "burgerservicenummer": {
      "pattern": "[0-9]{9}",
      "type": "string",
      "description": "burgerservicenummer",
